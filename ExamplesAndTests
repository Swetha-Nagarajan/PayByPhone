#Fisrt example usage of module

module "aws_resource_naming" {
  source = "./aws_resource_naming"

  domain      = "reporting"
}

#Second Example 

module "aws_resource_naming" {
  source = "./aws_resource_naming"

  domain      = "data"
  service_name = "foobar"
}

# Third Example

module "aws_resource_naming" {
  source = "./aws_resource_naming"

  domain      = "reporting"
  environment = "development"
}


#Fourth example

module "aws_resource_naming" {
  source = "./aws_resource_naming"

  domain      = "data"
  environment = "production"
  service_name = "foobar"
}


